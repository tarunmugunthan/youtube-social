{
    "manifest_version": 3,
    "name":"test extension",
    "version": "0.0.1",
    "description":"I'm learning to build extensions",
    "background":
    {
        "service_worker": "background.js", "type": "module"
    },

   "content_scripts": [
       {
           "matches": ["https://www.youtube.com/*"],
           "js": ["content.js", "jquery.js"],
           "css": ["style.css"]
       }
   ],

   "permissions": [
       "tabs", "webNavigation", "scripting", "activeTab"
   ],

   "host_permissions": [
       "<all_urls>"
   ],

   "action": {
        "default_icon": {                
        "32": "Star.png"      
        },
        "default_title": "Youtube Social",  
        "default_popup": "popup.html"    
    }
}
