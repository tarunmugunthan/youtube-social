{
    "manifest_version": 3,
    "name":"test extension",
    "version": "0.0.1",
    "description":"I'm learning to build extensions",
    "background":
    {
        "service_worker": "background.js"
    },

   "content_scripts": [
       {
           "matches": ["https://www.youtube.com/"],
           "js": ["content.js"],
           "css": ["style.css"]
       },
       {
           "matches": ["https://www.youtube.com/watch?*"],
           "run_at": "document_end",
           "js": ["contentwatch.js"],
           "css": ["stylewatch.css"]
       }
   ],

   "permissions": [
       "tabs", "webNavigation", "scripting", "activeTab"
   ],

   "host_permissions": [
       "<all_urls>"
   ]
}